{
  "expo": {
    "name": "curveWallet",
    "description": "Mobile wallet for the CryptoCurve environment",
    "slug": "curveWallet",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cryptocurve.curvewallet"
    },
    "android": {
      "package": "com.cryptocurve.curvewallet"
    }
  }
}
